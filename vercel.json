{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@microlink/vercel-builder-playwright"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
