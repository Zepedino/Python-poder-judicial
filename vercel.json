{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "env": {
          "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "1"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
